{
	"coc.preferences.bracketEnterImprove": true,
	"coc.preferences.jumpCommand": "tab drop",
	"suggest.maxPreviewWidth": 50,
	"suggest.maxCompleteItemCount": 30,
	"list.maxPreviewHeight": 15,
	"suggest.enablePreview": true,
	"suggest.completionItemKindLabels": {
		"keyword": "\uf1de",
		"variable": "\ue79b",
		"value": "\uf89f",
		"operator": "\u03a8",
		"function": "\u0192",
		"reference": "\ufa46",
		"constant": "\uf8fe",
		"method": "\uf09a",
		"struct": "\ufb44",
		"class": "\uf0e8",
		"interface": "\uf417",
		"text": "\ue612",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"module": "\uf40d",
		"color": "\ue22b",
		"property": "\ue624",
		"field": "\uf9be",
		"unit": "\uf475",
		"event": "\ufacd",
		"file": "\uf723",
		"folder": "\uf114",
		"snippet": "\ue60b",
		"typeParameter": "\uf728",
		"default": "\uf29c"
	},
	// coc-explorer
	"explorer.width": 30,
	"explorer.toggle": true,
	"explorer.icon.enableNerdfont": true,
	"explorer.previewAction.onHover": false,
	"explorer.keyMappings": {
		"<cr>": ["expandable?", "expand", "open"],
		"v": "open:vsplit"
	},
	"explorer.file.column.indent.indentLine": true,
	"explorer.buffer.showHiddenBuffers": true,
	"tslint.exclude": [ "node_modules" ],
	"tslint.enable": true,
	"typescript.suggest.enabled": true,
	"explorer.file.hiddenRules": {
		"extensions": [
			"o",
			"a",
			"obj",
			"pyc"
		],
		"filenames": [],
		"patternMatches": [
			"^\\."
		]
	},
	"coc.preferences.colorSupport": true,
	"coc.preferences.hoverTarget": "float",
	"coc.preferences.enableFloatHighlight": true,
	"coc.preferences.extensionUpdateCheck": "weekly",
}
